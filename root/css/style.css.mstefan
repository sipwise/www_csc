	margin: 0 5px 3px 0;
}
.smsversenden #smsdate {
	margin: 0 10px 0 0;
}
.smsversenden #smstimehour {
	margin: 0 3px 0 10px;
}
.smsversenden #smstimeminute {
	margin: 0 10px 0 3px;
}
#main .smsliste .smsmessage {
	background: #FFFDE1;
