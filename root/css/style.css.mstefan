	text-align: right;
}
.voicemaillistebig .call img {
	margin: 3px 0 0 0;
}
.voicemaillistebig .anrufloeschen img {
	margin: 3px 0 0 0;
}
.voicenmailnummer {
	margin: 0 0 10px 0;
	padding: 0px;
}
.voicenmailnummer select {
	width: 191px;
	border: 1px solid #C5CDD2;
}

/* 
	**********************
	***** 6.6 Status *****
