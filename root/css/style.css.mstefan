*/

.voicemailliste {
	margin: 0 0 10px 0;
	border-top: 1px solid #D0DCDF;
	width: 100%;
	float: left;
}
.voicemailliste li {
	margin: 0px;
	padding: 7px 0 5px 0;
	color: #999999;
	line-height: 14px;
	border-bottom: 1px solid #D0DCDF;
