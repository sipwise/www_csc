[% TAGS [- -] -%]
[% PROCESS '/usr/lib/ngcp-ngcpcfg/get_hostname'; hostname = out -%]
[% argv.host=hostname; argv.role='mgmt';
  PROCESS '/usr/lib/ngcp-ngcpcfg/has_role';
  is_mgmt = out
-%]
[% IF is_mgmt -%]
RUN_DAEMON=[% www_csc.enable %]
[% ELSE -%]
RUN_DAEMON="no"
[% END -%]
NPROC=[% www_csc.fastcgi_workers %]
